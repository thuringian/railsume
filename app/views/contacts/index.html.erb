<h2>Contact</h2>
<div class="row">     
    <div class="col-xs-12 col-md-12">	
  		<% @contacts.each do |contact| %>
    	   	
      		<%= link_to contact.link, :class => "btn btn-social-icon btn-lg btn-" + contact.title, :role => "button" do %>
      			<i class="fa fa-<%= contact.title %>"></i>
      		<% end %>
   
  		<% end %>
	</div>
</div>